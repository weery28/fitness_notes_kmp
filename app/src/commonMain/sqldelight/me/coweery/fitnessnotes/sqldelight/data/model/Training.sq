CREATE TABLE training (
id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL ,
name TEXT NOT NULL,
creationDate INTEGER,
date INTEGER,
isSynced INTEGER as Boolean,
serverId INTEGER
);

selectAll:
SELECT *
FROM training;

findById:
SELECT *
FROM training
WHERE id = ?;

insert:
INSERT INTO training(name, creationDate, date, isSynced, serverId)
VALUES (?, ?, ?,?,?);

lastInserRowId:
SELECT last_insert_rowid() FROM training;
